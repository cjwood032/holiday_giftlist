<%= form_for ([user]), html: {:id => "friend-gift-form"} do |f| %>
    <div class="form-group">
    <%= f.collection_radio_buttons(:friend_ids, @user.friends, :id, :name) %>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    
    <%= f.submit name="see gifts"%>
    <div id="friendArea"></div>
<%end%>